    Date.prototype.Format = function (fmt) { //author: meizz   
        var o = {
            "M+": this.getMonth() + 1,                 //月份   
            "d+": this.getDate(),                    //日   
            "h+": this.getHours(),                   //小时   
            "m+": this.getMinutes(),                 //分   
            "s+": this.getSeconds(),                 //秒   
            "q+": Math.floor((this.getMonth() + 3) / 3), //季度   
            "S": this.getMilliseconds()             //毫秒   
        };
        if (/(y+)/.test(fmt))
            fmt = fmt.replace(RegExp.$1, (this.getFullYear() + "").substr(4 - RegExp.$1.length));
        for (var k in o)
            if (new RegExp("(" + k + ")").test(fmt))
                fmt = fmt.replace(RegExp.$1, (RegExp.$1.length == 1) ? (o[k]) : (("00" + o[k]).substr(("" + o[k]).length)));
        return fmt;
    };

    function countDown() {
        currentTime = Number(currentTime) + 1e3,

        $("#xb-q-jis .xb-gu-mead").each(function () {
            var $time = $(this),
            dataValue = $time.data("time").split("§");
            if (2 == dataValue.length) {
                var beginTime = new Date(Trim(dataValue[0], "0").replace(/-/g, "/")),
                endTime = new Date(Trim(dataValue[1], "0").replace(/-/g, "/"));

                if (currentTime >= endTime || currentTime < beginTime) {
                    $("#xb-q-jis").hide();
                }
                else {
                    currentTime >= beginTime && endTime > currentTime && timechange(endTime, $time)
                }
            }
        }),
        window.setTimeout("countDown()", 1e3);
    }

    function timechange(endTime, obj) {
        var theDay = new Date(endTime),
        seconds = (theDay - currentTime) / 1e3,
        minutes = Math.floor(seconds / 60),
        hours = Math.floor(minutes / 60),
        days = Math.floor(hours / 24),
        CDay = days,
        CHour = hours % 24,
        CMinute = minutes % 60,
        CSecond = seconds % 60;
        CDay = 10 > CDay ? "0" + CDay : CDay,
        CHour = 10 > CHour ? "0" + CHour : CHour,
        CMinute = 10 > CMinute ? "0" + CMinute : CMinute,
        CSecond = 10 > CSecond ? "0" + CSecond : CSecond,
        obj.html("<span class='ji_span1d'>" + CDay + "</span><em>天</em><span class='ji_span1d'>" + CHour + "</span><em>时</em><span class='ji_span2d'>" + CMinute + "</span><em>分</em><span class='ji_span3d'>" + CSecond + "</span><em>秒</em>");
    }

    function Trim(str, is_global) {
        var result;
        return result = str.replace(/(^\s+)|(\s+$)/g, ""),
        "g" == is_global.toLowerCase() && (result = result.replace(/\s/g, "")),
        result
    }
